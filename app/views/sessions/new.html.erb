<h1>Log in</h1>
<div class="form-group">
 <%= form_tag sessions_path do %>
  <%= text_field_tag :email %>
  <br>
  <%= password_field_tag :password %>
  <br>
  <%= submit_tag 'Log in', class: 'btn btn-success' %>
 <% end %>
</div>
<h2> New user? </h2>
 <%= link_to 'Sing Up', sing_up_path, class: 'btn btn-success' %>
